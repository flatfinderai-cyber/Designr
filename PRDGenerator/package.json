{
  "name": "prd-generator",
  "version": "1.0.0",
  "description": "Interactive PRD.json generator for Ralph-loop automation. Q&A-driven workflow to create Product Requirements Documents optimized for AI agent execution.",
  "license": "MIT",
  "author": {
    "name": "Designr Team",
    "url": "https://github.com/flatfinderai-cyber"
  },
  "homepage": "https://github.com/flatfinderai-cyber/Designr/tree/main/PRDGenerator",
  "repository": {
    "type": "git",
    "url": "https://github.com/flatfinderai-cyber/Designr.git",
    "directory": "PRDGenerator"
  },
  "bugs": {
    "url": "https://github.com/flatfinderai-cyber/Designr/issues"
  },
  "keywords": [
    "prd",
    "product-requirements",
    "ralph-loop",
    "automation",
    "ai-agents",
    "next.js",
    "react",
    "typescript",
    "q&a",
    "interactive"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./store": {
      "types": "./dist/lib/store.d.ts",
      "default": "./dist/lib/store.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "DISTRIBUTION-INFO.md",
    "package.json"
  ],
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build && tsc --emitDeclarationOnly",
    "start": "next start",
    "lint": "eslint . --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "tailwindcss": "^3.4.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.1",
    "lucide-react": "^0.308.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.0",
    "prettier": "^3.1.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
